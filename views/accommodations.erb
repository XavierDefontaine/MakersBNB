<body>
<% @accommodation_list.each do |space| %>

<div class="card" style="width: 18rem;">
  <img class="card-img-top" src="<%=space.image_url%>" alt="A lovely property">
  <div class="card-body">
    <h4 class="card-title"><%= space.name%></h4>
    <h4 class="card-text"><%= space.description%></h4>
    <a href="/accommodations/<%= space.id%>" class="btn btn-primary">See more</a><hr>
  </div>
</div>
<% end %>
</body>
